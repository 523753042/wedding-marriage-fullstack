.title-box{
  padding-top: 50rpx;
}
.title{
  text-align: center;
  padding: 20rpx;
  position: relative;
  line-height: 1;
  font-size: 40rpx;
  height: 40rpx;
}
.head-fixed{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1;
}
.back{
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  font-size: 60rpx;
  line-height: 80rpx;
  width: 120rpx;
  text-align: center;
}